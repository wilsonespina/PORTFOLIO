{
  "name": "react-webpack-setup",
  "version": "1.0.0",
  "description": "A React and Webpack setup for WDI",
  "main": "server.js",
  "license": "MIT",
  "scripts": {
    "start": "yarn build && node index",
    "start:client": "webpack-dev-server",
    "start:server": "nodemon",
    "build": "webpack -p",
    "seed": "node db/seeds",
    "test:client": "node_modules/.bin/_mocha test/client/helper \"test/client/**/*_test.js\"",
    "test:server": "node_modules/.bin/_mocha \"test/api/**/*_test.js\"",
    "test": "yarn test:server && yarn test:client"
  },
  "dependencies": {
    "body-parser": "^1.18.1",
    "bootstrap-css-only": "^3.3.7",
    "express": "^4.15.4",
    "font-awesome": "^4.7.0",
    "moment": "^2.19.4",
    "mongoose": "^4.13.9",
    "mongoose-unique-validator": "^1.0.6",
    "morgan": "^1.8.2",
    "newrelic": "^2.6.0",
    "prop-types": "^15.6.0",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^15.6.1",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.2.2",
    "react-scroll": "^1.7.4",
    "react-simple-sidenav": "^0.1.6",
    "request": "^2.34",
    "request-promise": "^4.2.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.0"
